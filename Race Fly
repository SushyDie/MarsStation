--function

local function click(a)
    game:GetService("VirtualInputManager"):SendMouseButtonEvent(a.AbsolutePosition.X+a.AbsoluteSize.X/2,a.AbsolutePosition.Y+50,0,true,a,1)
    game:GetService("VirtualInputManager"):SendMouseButtonEvent(a.AbsolutePosition.X+a.AbsoluteSize.X/2,a.AbsolutePosition.Y+50,0,false,a,1)
end

function AddTrail()
    game:GetService("Players").LocalPlayer.TrailsData.Equipped.Value = _G.SelectTrail
    end

function CollectOrb22() --ty Trohten2#2970!
    
    game.Workspace.Camera.ChildAdded:Connect(function(Part)
        if _G.CollectOrb2 == true then
    for i, Orb in pairs(game:GetService("Workspace").Camera:GetChildren()) do
        
        
        if Orb:IsA("Part") and Orb.Orb.Item.Image ~= "rbxassetid://11612382598" or Orb.Orb.Item.Image ~= "rbxassetid://11411247929" then --rbxassetid://11411247929
            local Player = game:GetService("Players").LocalPlayer
            local Character = Player.Character or Player.CharacterAdded:Wait()
            Orb.CanCollide = false 
            Orb.CFrame = CFrame.new(Character.PrimaryPart.Position)
        end
        
    end
    
end
end)
end



function Optimethod1()
    while _G.OptiFarm1 == true do
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(232.0892791748047, 35, -894.423583984375)
        wait(0.2)
        repeat
            OptiFarm1 = OptiFarm1 + 1
           game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(160335.421875, 396.5972900390625, 104614.515625)
           wait()
        until OptiFarm1 == _G.OptiFarmValue
        wait()
        OptiFarm1 = 0
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(200, -1, 104659)
        wait(4)
    end
end

