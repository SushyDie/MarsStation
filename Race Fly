local Section = AutoFarm:CreateSection("Reset")

local Button = AutoFarm:CreateButton({
	Name = "Reset character",
	Callback = function()
        for i, v in pairs (game:GetService("Players"):GetPlayers()) do
            v.Character:FindFirstChild("Humanoid").Health = 0
        end
    end,
})

local credit = credit:CreateParagraph({Title = "Credit", Content = "SushyDie#8830"})

local credit = credit:CreateButton({
	Name = "Destroy the interface",
	Callback = function()
        Rayfield:Destroy()
    end
})
 
