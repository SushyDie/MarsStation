function Barrier()
    while _G.Barrier == true do
        game:GetService("Workspace").Worlds.FindForChild("1").MainBarrier.CanCollide = false
        game:GetService("Workspace").Worlds.FindForChild("2").MainBarrier.CanCollide = false
        game:GetService("Workspace").Worlds.FindForChild("3").MainBarrier.CanCollide = false
        wait(1)
        
     end
end

function AutoEquipB()
    while _G.AutoEquipBest == true do
local args = {
    [1] = "EquipBest",
    [2] = {
        ["PetID"] = 0
    }
}

game:GetService("ReplicatedStorage").RemoteEvents.PetActionRequest:InvokeServer(unpack(args))

        wait(1)
    end
end

